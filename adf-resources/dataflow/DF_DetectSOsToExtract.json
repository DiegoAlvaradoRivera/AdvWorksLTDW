{
	"name": "DF_DetectSOsToExtract",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AdvWorksLTQuery",
						"type": "DatasetReference"
					},
					"name": "SOsInSQLDB"
				},
				{
					"dataset": {
						"referenceName": "AdvWorksLTQuery",
						"type": "DatasetReference"
					},
					"name": "SOsInDW"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SalesOrdersToExtract",
						"type": "DatasetReference"
					},
					"name": "SaveSOsToExtract"
				}
			],
			"transformations": [
				{
					"name": "InnerJoin"
				},
				{
					"name": "KeepSOsNotInDW"
				},
				{
					"name": "SOsToExtract"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SalesOrderID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select SalesOrderID\\nfrom SalesLT.SalesOrderHeader\\nwhere Status = 5',",
				"     format: 'query') ~> SOsInSQLDB",
				"source(output(",
				"          SalesOrderID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select distinct SalesOrderID \\nfrom presentation.FactSalesOrders',",
				"     format: 'query') ~> SOsInDW",
				"SOsInSQLDB, SOsInDW join(SOsInSQLDB@SalesOrderID == SOsInDW@SalesOrderID,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> InnerJoin",
				"InnerJoin filter(isNull(SOsInDW@SalesOrderID)) ~> KeepSOsNotInDW",
				"KeepSOsNotInDW select(mapColumn(",
				"          SalesOrderID = SOsInSQLDB@SalesOrderID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SOsToExtract",
				"SOsToExtract sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     preSQLs:['truncate table integration.SalesOrdersToExtract'],",
				"     saveOrder: 1,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          SalesOrderId = SalesOrderID",
				"     )) ~> SaveSOsToExtract"
			]
		}
	}
}