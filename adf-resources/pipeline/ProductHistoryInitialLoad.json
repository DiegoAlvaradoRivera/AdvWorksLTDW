{
    "name": "ProductHistoryInitialLoad",
    "properties": {
        "activities": [
            {
                "name": "Stage Initial CT File",
                "type": "Copy",
                "dependsOn": [
                    {
                        "activity": "Set CT File Name",
                        "dependencyConditions": [
                            "Succeeded"
                        ]
                    }
                ],
                "policy": {
                    "timeout": "0.12:00:00",
